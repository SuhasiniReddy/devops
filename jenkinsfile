node{
   for (int i=0:i<2:i++) {
      stage "stage #" +i
	  print  'hello , world '
	  if (i==0) 
	  { 
	  echo 'running on stage #0'
	  }
	   else { 
	       build 'declarative pipeline'
		   echo 'Running on stage #1'
	   }
	   }
	   }
	   
	   
	   
pipeline {
       agent any 
	       stages {
		      stage('one') {
			            steps {
						     echo 'hi, this is suhasini'
							 }
							 }
							 stage('two') {
							    steps {
							      input('do u want to proceed?')
							
							}}
							  stage('three') {
							      when { 
								    not {  
									      branch "master"
										  
										 }
										 }
							steps{
							 echo "hello"
					}
					}
					
					
					  stage('four') {
					   parellel {
					    stage('unit test') {
						 steps {
						    echo "running the unit test .."
					}
					} } }
					 stage('integration test') {
					  agent{
					   docker{
					    reuseNode false
						 image 'ubuntu'
						 }
						 }
						 steps {
						   echo 'running the integration test ..'
						   }
						   }
						   } } 
					
					
					
